+++ ccl-0.1.1/demo/test_entry.c	2023-11-30 15:29:39.967848752 +0800
@@ -0,0 +1,25 @@
+#include <stdio.h>
+
+#include "ccl/ccl.h"
+
+int test_ccl(const char *conf_path)
+{
+    struct ccl_t 			config;
+    const struct ccl_pair_t 	*iter;
+
+    config.comment_char = '#';
+    config.sep_char = '=';
+    config.str_char = '"';
+    if (conf_path == NULL) {
+        conf_path = "./simple.conf";
+    }
+    ccl_parse(&config, conf_path);
+
+    while((iter = ccl_iterate(&config)) != 0) {
+        printf("(%s,%s)\n", iter->key, iter->value);
+    }
+
+    ccl_release(&config);
+
+    return 0;
+}
